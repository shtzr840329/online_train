<!DOCTYPE html>
<html>
<head>
  <title>上海高等职业教育师资公共培训</title>
  <%= stylesheet_link_tag    'admin_panel', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'admin_panel', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body class="<%= controller.controller_path.gsub('/','__') + '__' + controller.action_name %>">
  <%= render "admin_panel/share/header"%>
  <div class="row">
    <div class="col-sm-2">
       <%= render "admin_panel/share/left_bar"%>
    </div>
    <div class="col-sm-10">
       <%= yield %>
    </div>
  </div>

  <!-- flash notice -->
  <%= render :partial => "admin_panel/share/flash_message" %>
  <!-- flash notice -->
  <%= render "admin_panel/share/footer"%>
</body>
</html>
