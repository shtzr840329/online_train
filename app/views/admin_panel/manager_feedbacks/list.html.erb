<div class="main-content">
	<div class="panel panel-default">
	  <div class="panel-heading panel-active">搜索栏</div>
	  <div class="panel-body panel-default-bg">
	    <div class="row">
	      <%= form_tag list_admin_panel_manager_feedbacks_path, method: :get do %>
					<div class="col-sm-3">
				    <div class="form-group">
				     <%= text_field_tag :q, nil, class: "form-control", placeholder: '请输入关键字' %>
					  </div>
					</div>
					<div class="col-sm-3">
				    <div class="form-group">
				      <%= submit_tag "搜索", class: "btn primary" %>
					  </div>
					</div>
				<% end %>
			</div>
		</div>
	</div>
	<div class="panel panel-default">
	  <!-- <div class="panel-heading panel-active">评审项目</div> -->
	  <!-- 培训机构的权限是新建申报项目 -->
	  <div class="panel-heading panel-active" style="overflow: hidden;">
	    <span class="pull-left">负责人反馈</span>
	  </div>
	  <div class="panel-body panel-default-bg">
	    <table class="table table-condensed">
	    	<thead>
	    	  <tr>
	    	  	<th class="text-center">负责培训班</th>
	    	  	<th class="text-center">负责人</th>
	    	  	<th class="text-center">承办单位</th>
	    	  	<th class="text-center">总学时</th>
		    		<th class="text-center">总收支</th>
		    		<th class="text-center">总收支明细</th>
		    		<th class="text-center">反馈内容</th>
		    		<th class="text-center">其他情况/备注</th>
		    		<th class="text-center">操作</th>
	    	  </tr>
	    	</thead>
	    	<tbody>
	    	  <%= render partial: 'manager_feedback', locals: {search: @search}%>
		   	</tbody>
	    </table>
	    <div class="text-center">
	    	<%= paginate @manager_feedbacks %>
	    </div>
	  </div>
	</div>
</div>